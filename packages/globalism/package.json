{
  "name": "globalism",
  "version": "0.1.0",
  "description": "Comprehensive country data and utilities for phone numbers, addresses, and more",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "country",
    "phone",
    "validation",
    "formatting",
    "international",
    "globalization",
    "i18n"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/globalism.git",
    "directory": "packages/globalism"
  }
}